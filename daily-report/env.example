# Database Configuration
# Copy this file to .env and fill in your actual credentials

# SQLAlchemy connection string for your database
# Examples:
# MySQL: mysql+pymysql://user:password@host:port/database
# SQL Server: mssql+pyodbc://user:password@host:port/database?driver=ODBC+Driver+17+for+SQL+Server
# PostgreSQL: postgresql://user:password@host:port/database
SQLALCHEMY_URL=mysql+pymysql://username:password@localhost:3306/database_name

# Output file name (optional, defaults to DailyReport.html)
OUTPUT_HTML=DailyReport.html

# Day banner text (optional, shows current date/info)
DAY_BANNER=Daily Report for Pacific Coast Title